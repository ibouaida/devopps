    agent any

    stages {
        stage('Build') {
            steps {
                echo 'ici on fait le build du code'
            }
        }
        stage('Test') {
            steps {
                echo 'on fait un test integration continue'
            }
        }
        stage('Deploy') {
            steps {
                echo 'le deploiement de application'
            }
        }
    }
}